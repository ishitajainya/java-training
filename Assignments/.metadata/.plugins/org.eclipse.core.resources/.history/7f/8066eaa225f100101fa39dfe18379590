package com.lab_assignment_1;

public class Q1 {

	public static void main(String[] args) {
		int a = 0, b = 1;
		int arr[] = new int[20];
		int sum = 0, avg = 0;
		arr[0] = a; arr[1] = b;
		System.out.println("The series is : ");
		for(int i = 2; i < 20; i++) {
			int c = a + b;
			arr[i] = c;
			a = b;
			b = c;
			sum += arr[i];
			System.out.print(arr[i] + " ");
		}
		avg = sum / 20;
		System.out.println();
		System.out.println("The average is " + avg);
	}

}
