package com.lab_assignment_1;

public class Q1 {

	public static void main(String[] args) {
		int a = 0, b = 1;
		int arr[] = new int[20];
		arr[0] = a, arr[1] = b;
		for(int i = 2; i < 20; i++) {
			int c = a + b;
			arr[i] = c;
			a = b;
			b = c;
		}
		int sum = 0, avg = 0;
		for(int i = 0; i < 20; i++) {
			sum += arr[i];
			System.out.println(arr[i], " ");
		}
		avg = sum / 20;
		System.out.println(avg);
	}

}
